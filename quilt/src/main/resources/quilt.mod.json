{
  "schema_version": 1,
  "mixin": [
    "auto_crouch.mixins.json",
    "auto_crouch-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "auto_crouch",
    "version": "${version}",
    "metadata": {
      "name": "Auto Crouch",
      "description": "Automatically crouches when opening a GUI",
      "contributors": {
        "MrMelon54": "Owner"
      },
      "contact": {
        "homepage": "https://mrmelon54.com/minecraft/auto-crouch",
        "sources": "https://github.com/MrMelon54/auto_crouch",
        "issues": "https://github.com/MrMelon54/auto_crouch/issues"
      },
      "license": "GPL-3.0-only",
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.mrmelon54.AutoCrouch.quilt.AutoCrouchQuilt"
      ],
      "modmenu": [
        "com.mrmelon54.AutoCrouch.fabriclike.ModMenuIntegration"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20"
      },
      {
        "id": "architectury",
        "version": ">=9.0.0"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}
